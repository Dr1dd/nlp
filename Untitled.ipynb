   "source": [
    "import spacy\n",
    "nlp = spacy.load('en_core_web_sm')\n",
    "\n",
    "gpe = []\n",
    "loc = []\n",
    "\n",
    "\n",
    "doc = nlp(open('text.txt').read())\n",
    "for ent in doc.ents:\n",
    "    if (ent.label_ == 'GPE'):\n",
    "        gpe.append(ent.text)\n",
    "    elif (ent.label_ == 'LOC'):\n",
    "        loc.append(ent.text)\n",
    "\n",
    "print(gpe, loc)\n"
   ]
